    "Phone": {
      "type": "input",
      "element_name": "phone",
      "location": [
        {
          "value": "main"
        }
      ],
      "required": "yes",
      "show_column": "yes"
    },
    "Question / Comment": {
      "type": "textarea",
      "element_name": "question",
      "location": [
